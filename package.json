{
  "name": "thecore",
  "displayName": "Thecore",
  "description": "A Ruby on Rails Framework aimed at fast model driven API and Backend.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "thecore.setupDevcontainer",
      "title": "Thecore 3: Setup Devcontainer"
    },{
      "command": "thecore.createApp",
      "title": "Thecore 3: Create an App"
    },{
      "command": "thecore.releaseApp",
      "title": "Thecore 3: Release this App"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
